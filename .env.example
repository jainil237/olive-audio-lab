# Rename this file to .env and fill the values before running the app.
# Vite will expose variables prefixed with VITE_. Never commit real secrets.

# Stripe publishable key (starts with pk_test_ or pk_live_)
VITE_STRIPE_PUBLISHABLE_KEY=

# Razorpay key ID (public identifier)
VITE_RAZORPAY_KEY_ID=

# Firebase configuration
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=
